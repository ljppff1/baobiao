<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_a"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/fra_indexs_top"
        android:layout_width="match_parent"
        android:layout_height="@dimen/DIMEN_96PX"
        android:background="@color/indexs22" >

        <ImageView
            android:id="@+id/act_iv_viewss"
            android:layout_width="@dimen/DIMEN_43PX"
            android:layout_height="@dimen/DIMEN_43PX"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/DIMEN_35PX"
            android:adjustViewBounds="true"
            android:src="@drawable/fanhui3" />

        <TextView
            android:id="@+id/act_tv_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="Soloman个人资料"
            android:textColor="@color/color_a"
            android:textSize="19sp" >
        </TextView>
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/DIMEN_40PX"
        android:layout_marginRight="@dimen/DIMEN_40PX"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/act_rl_s"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/DIMEN_40PX"
            android:paddingTop="@dimen/DIMEN_40PX" >

            <LinearLayout
                android:id="@+id/act_ll_individual_zjzl"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/DIMEN_40PX"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="证件"
                    android:textColor="@color/black"
                    android:textSize="17sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/DIMEN_20PX"
                    android:text="资料"
                    android:textColor="@color/black"
                    android:textSize="17sp" />
            </LinearLayout>

            <View
                android:id="@+id/act_v_individual"
                android:layout_width="@dimen/DIMEN_1PX"
                android:layout_height="@dimen/DIMEN_122PX"
                android:layout_toRightOf="@id/act_ll_individual_zjzl"
                android:background="@color/cc" >
            </View>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/DIMEN_40PX"
                android:layout_toRightOf="@id/act_v_individual"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/act_tv_set_Certificates_number"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="证件编号"
                    android:textColor="@color/color_e7"
                    android:textSize="17sp" />

                <TextView
                    android:id="@+id/act_tv_set_certificates_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/DIMEN_20PX"
                    android:text="证件类型"
                    android:textColor="@color/color_e7"
                    android:textSize="17sp" />
            </LinearLayout>

            <ImageView
                android:layout_width="@dimen/DIMEN_30PX"
                android:layout_height="@dimen/DIMEN_30PX"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:adjustViewBounds="true"
                android:src="@drawable/s" >
            </ImageView>
        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="@color/color_e7" />

        <RelativeLayout
            android:id="@+id/act_rl_ss"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/DIMEN_40PX"
            android:paddingTop="@dimen/DIMEN_40PX" >

            <LinearLayout
                android:id="@+id/act_ll_individual_zjzl1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/DIMEN_40PX"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="银行"
                    android:textColor="@color/black"
                    android:textSize="17sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/DIMEN_20PX"
                    android:text="信息"
                    android:textColor="@color/black"
                    android:textSize="17sp" />
            </LinearLayout>

            <View
                android:id="@+id/act_v_individuals"
                android:layout_width="@dimen/DIMEN_1PX"
                android:layout_height="@dimen/DIMEN_122PX"
                android:layout_toRightOf="@id/act_ll_individual_zjzl1"
                android:background="@color/cc" >
            </View>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/DIMEN_40PX"
                android:layout_toRightOf="@id/act_v_individuals"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/act_tv_set_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="银行名称"
                    android:textColor="@color/black"
                    android:textSize="17sp" />

                <TextView
                    android:id="@+id/act_tv_set_names"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/DIMEN_20PX"
                    android:text="网点名称"
                    android:textColor="@color/black"
                    android:textSize="17sp" />
            </LinearLayout>

            <ImageView
                android:layout_width="@dimen/DIMEN_30PX"
                android:layout_height="@dimen/DIMEN_30PX"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:adjustViewBounds="true"
                android:src="@drawable/s" >
            </ImageView>
        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="@color/color_e7" />

        <RelativeLayout
            android:id="@+id/act_rl_sss"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/DIMEN_40PX"
            android:paddingTop="@dimen/DIMEN_40PX" >

            <LinearLayout
                android:id="@+id/act_ll_individual_zjzlss"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/DIMEN_40PX"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="联系"
                    android:textColor="@color/black"
                    android:textSize="17sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/DIMEN_20PX"
                    android:text="信息"
                    android:textColor="@color/black"
                    android:textSize="17sp" />
            </LinearLayout>

            <View
                android:id="@+id/act_v_individualss"
                android:layout_width="@dimen/DIMEN_1PX"
                android:layout_height="@dimen/DIMEN_122PX"
                android:layout_toRightOf="@id/act_ll_individual_zjzlss"
                android:background="@color/cc" >
            </View>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/DIMEN_40PX"
                android:layout_toRightOf="@id/act_v_individualss"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/act_tv_set_namess"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="名称"
                    android:textColor="@color/black"
                    android:textSize="17sp" />

                <TextView
                    android:id="@+id/act_tv_set_phone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/DIMEN_20PX"
                    android:text="号码"
                    android:textColor="@color/black"
                    android:textSize="17sp" />
            </LinearLayout>

            <ImageView
                android:layout_width="@dimen/DIMEN_30PX"
                android:layout_height="@dimen/DIMEN_30PX"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:adjustViewBounds="true"
                android:src="@drawable/s" >
            </ImageView>
        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="@color/color_e7" />

        <RelativeLayout
            android:id="@+id/act_rl_ssss"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/DIMEN_80PX"
            android:paddingTop="@dimen/DIMEN_80PX" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="特卫认证"
                android:textColor="@color/black"
                android:textSize="17sp" />

            <ImageView
                android:id="@+id/set_iv_"
                android:layout_width="@dimen/DIMEN_30PX"
                android:layout_height="@dimen/DIMEN_30PX"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:adjustViewBounds="true"
                android:src="@drawable/s" >
            </ImageView>

            <TextView
                android:id="@+id/set_tv_"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/DIMEN_10PX"
                android:layout_toLeftOf="@+id/set_iv_"
                android:text="未认证"
                android:textColor="@color/black"
                android:textSize="17sp" />

            <ImageView
                android:id="@+id/set_iv__"
                android:layout_width="@dimen/DIMEN_30PX"
                android:layout_height="@dimen/DIMEN_30PX"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/DIMEN_6PX"
                android:layout_toLeftOf="@+id/set_tv_"
                android:adjustViewBounds="true"
                android:src="@drawable/yirenzheng3" >
            </ImageView>
        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="@color/color_e7" />
    </LinearLayout>

</LinearLayout>